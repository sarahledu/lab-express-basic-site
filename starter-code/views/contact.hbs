<div class="contact-page">
    <h2>Contact us</h2>
    <div class="">

        <form action="" class="contact-form">
            <div class="formulaire">
                <div class="label">
                    <label for="name">Name: </label>
                    <label for="email">Email: </label>
                    <label for="number">Number: </label>
                    <label for="house">House you'd like to rent: </label>
                    <label for="date">Date from: </label>
                    <label for="date">Date to: </label>
                    <label for="message">Message: </label>
                </div>
                <div class="input">
                    <input type="text" id="name">
                    <input type="email" id="email">
                    <input type="text" id="number">
                    <div class="house-choice">
                        <input type="radio" id="house1" value="Guilvinec">
                        <label for="house2">Guilvinec</label>
                        <input type="radio" id="house2" value="Plozevet">
                        <label for="house3">Plozevet</label>
                        <input type="radio" id="house3" value="Both">
                        <label for="house">Both</label>
                    </div>
                    <input type="date" id="date">
                    <input type="date" id="date">
                    <textarea rows="5" id="message"></textarea>
                </div>
            </div>
            <button class="btn-ok">OK</button>
        </form>

    </div>